{
  "name": "emona-textile-frontend",
  "version": "1.0.0",
  "private": true,
  "license": "BSD 3-Clause",
  "repository": {
    "type": "git",
    "url": "git@github.com:mariukhin/emona_textile.git"
  },
  "author": "Maksym Mariukhin",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve -c ./webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack -c ./webpack/webpack.prod.js",
    "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json",
    "lint": "eslint \"./src/**/*.ts\" \"./src/**/*.tsx\" --fix",
    "precommit:lint": "eslint \"./src/**/*.ts\" \"./src/**/*.tsx\" -c precommit.eslint.json --no-eslintrc --fix",
    "precommit": "npm run precommit:lint && npm run typecheck && git add .",
    "prettier": "prettier --write \"./src/**/*.{js,jsx,css}\"",
    "typecheck": "tsc -p . --noEmit"
  },
  "alias": {
    "routing": "./src/routing",
    "pages": "./src/pages",
    "assets": "./src/assets"
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "ie >= 9"
  ],
  "devDependencies": {
