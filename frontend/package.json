    "@babel/preset-typescript": "7.16.7",
    "@types/google.maps": "^3.48.8",
    "@types/node": "^18.11.0",
    "@types/ramda": "^0.28.11",
    "@types/react": "^18.0.6",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^18.0.2",
    "@types/react-router": "^5.1.0",
    "@types/react-router-dom": "^5.1.0",
